{
  "name": "swr-login",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "build": "tsc",
    "run": "node ./lib/main.js",
    "package": "ncc build --source-map --license licenses.txt",
    "lint-create": "eslint --init",
    "lint": "eslint src",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "prepare": "ncc build src/index.js -o dist --source-map --license licenses.txt",
    "test": "jest --collectCoverage",
    "all": "npm run lint && npm run format && npm run build && npm run package"
  },
  "keywords": [
    "actions",
    "Huawei Cloud",
    "container",
    "github",
    "swr"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huaweicloud/swr-login.git"
  },
  "license": "Apache",
  "bugs": {
    "url": "https://github.com/huaweicloud/swr-login/issues"
  },
  "homepage": "https://github.com/huaweicloud/swr-login#readme",
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/io": "^1.0.2",
    "axios": "^0.21.1",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.32"
  },
  "devDependencies": {
    "@types/jest": "^28.1.4",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "@vercel/ncc": "^0.24.1",
    "eslint": "^7.32.0",
    "jest": "^28.1.2",
    "prettier": "2.7.1",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.2"
  }
}
